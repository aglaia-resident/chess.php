{
    "name": "aglaia-resident/chess.php",
    "type": "library",
    "description": "Fork of ryanhs/chess.php. chess.php is a PHP chess library that is used for chess move generation/validation, piece placement/movement, and check/checkmate/stalemate detection - basically everything but the AI. NOTE: this is a port of chess.js for php",
    "license": "MIT",
    "authors": [
        {
            "name": "ryan hs",
            "email": "mr.ryansilalahi@gmail.com"
        },
        {
            "name": "Aglaia",
            "email": "aglaia@noemail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "johnkary/phpunit-speedtrap": "^3.1",
        "phpunit/phpunit": "^9",
        "dms/phpunit-arraysubset-asserts": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Ryanhs\\Chess\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "fix-cs": "./vendor/bin/php-cs-fixer fix",
        "check-cs": "./vendor/bin/php-cs-fixer fix --dry-run"
    }
}
